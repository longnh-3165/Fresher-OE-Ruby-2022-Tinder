<table class="table table-striped table-borderless">
  <thead class="thead-dark">
    <tr>
      <th scope="col">#</th>
      <th scope="col"><%= t ".name" %></th>
      <th scope="col"><%= t ".gold" %></th>
      <th scope="col"><%= t ".act" %></th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <th scope="row"><%= user.id %></th>
        <td><%= user.name %></td>
        <td><%= user.type_of %></td>
        <td>
          <div class="btn-group" role="group" aria-label="User actions">
            <button class="btn btn-info"><%= t ".info" %></button>
            <button class="btn btn-warning"><%= t ".up" %></button>
            <button class="btn btn-danger"><%= t ".del" %></button>
          </div>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
